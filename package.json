{
  "name": "reachout-bot",
  "version": "0.1.0",
  "description": "Reach Out Bot MVP - NGO Outreach Campaign Automation",
  "main": "dist/backend/index.js",
  "scripts": {
    "dev": "npm run build && node dist/backend/index.js",
    "server": "npm run build && node dist/server/api.js",
    "build": "tsc",
    "test": "npm run build && node dist/tests/constraints.test.js",
    "test:watch": "npm run build && node --watch dist/tests/constraints.test.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rimraf dist",
    "local:reset": "rm -rf data/mock-data && echo 'Mock database reset'"
  },
  "keywords": [
    "ngo",
    "outreach",
    "automation",
    "email",
    "hubspot",
    "supabase"
  ],
  "author": "Dots Eco",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.1.4",
    "express": "^4.22.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
